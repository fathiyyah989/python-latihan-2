data=[1,4,3,0,8,6]
n=len(data)
print("data awal =",data)
tukar=5
while tukar>0:
    tukar=0
    for i in range(0,n-1):
        if data[i]>data[i+1]:
            s=data[i]
            data[i]=data[i+1]
            data[i+1]=s
            tukar=tukar+1
print("data akhir =",data)
